<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="configurationService"
		class="fr.gnss.constellation.ouranos.services.impl.OuranosConfigurationServiceImpl">
		<property name="validationService" ref="validationService"></property>
		<property name="executionDao" ref="executionDao"></property>
	</bean>

	<bean id="executionService"
		class="fr.gnss.constellation.ouranos.services.impl.OuranosExecutionServiceImpl">
		<property name="configurationService" ref="configurationService"></property>
	</bean>

	<bean id="validationService"
		class="fr.gnss.constellation.ouranos.services.impl.OuranosValidationServiceImpl">
	</bean>

</beans>