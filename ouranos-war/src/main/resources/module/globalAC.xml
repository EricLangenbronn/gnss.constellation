<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<import resource="classpath:/module/ouranos-dao.xml" />
	<import resource="classpath:/module/ouranos-services.xml" />

	<!-- Service Factory -->
	<bean id="serviceFactory"
		class="fr.gnss.constellation.ouranos.api.service.ServiceFactory">
		<property name="orbitsDataService">
			<ref bean="orbitsDataService" />
		</property>
		<property name="sateliteVisibleService">
			<ref bean="sateliteVisibleService" />
		</property>
		<property name="fluxService">
			<ref bean="fluxService" />
		</property>
	</bean>

</beans>