<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<import resource="classpath:/module/ouranos-dao.xml" />
	<import resource="classpath:/module/ouranos-services.xml" />

	<!-- Service Factory -->
	<bean id="ouranosServiceFactory"
		class="fr.gnss.constellation.ouranos.api.service.ServiceFactory">
		<property name="ouranosConfigurationService">
			<ref bean="configurationService" />           
		</property>
        <property name="ouranosExecutionService">
			 <ref bean="executionService" />
		</property>
        
       
	</bean>

</beans>